<h1> 编辑 </h1>

<%= simple_form_for [:admin, @job ] do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "交易下限" %>
<%= f.input :wage_upper_bound, :label => "交易上限" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
  <%= f.submit "发送" %>
<% end %>
